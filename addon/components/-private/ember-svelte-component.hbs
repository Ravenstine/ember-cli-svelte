{{#let (element this.tagName) as |wrappingElement|}}
  <wrappingElement ...attributes>
    {{this.svelteComponentAnchor}}
    {{did-insert this.insertSvelteComponent}}
    {{#if this.defaultSlotElement}}{{#in-element this.defaultSlotElement nextSibling=this.defaultSlotAnchor}}{{#if this.showsDefaultSlot}}{{yield}}{{/if}}{{/in-element}}{{/if}}
    {{#each this.argsValues as |value|}}
      {{helper this.updateSvelteComponentHelper}}
    {{/each}}
    {{will-destroy this.teardownSvelteComponent}}
  </wrappingElement>
{{/let}}
